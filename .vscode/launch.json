{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "consumer1",
      "program": "${workspaceFolder}/consumer/node/consumer1.js",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "runtimeArgs": [],
      "env": {},
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Python: PyConsumer",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/consumer/python/consumer1.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/consumer/python/.venv/bin/python"
    }
  ]
}